@page "/test"

<SfDialog Width="400px" IsModal="true" Visible="true" Header="Sveiki!">
    <DialogTemplates>
        <Content>Syncfusion darbojas ✔️</Content>
    </DialogTemplates>
</SfDialog>

<SfGrid DataSource="data" AllowPaging="true" Height="250px">
    <GridColumns>
        <GridColumn Field=@nameof(Row.Id) HeaderText="ID" Width="100"></GridColumn>
        <GridColumn Field=@nameof(Row.Name) HeaderText="Name"></GridColumn>
    </GridColumns>
</SfGrid>

@code {
    public class Row { public int Id { get; set; } public string Name { get; set; } = ""; }
    List<Row> data = new() { new(){Id=1, Name="A"}, new(){Id=2, Name="B"} };
}
